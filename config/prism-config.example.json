{
  "claude": {
    "apiKey": "your_anthropic_api_key_here",
    "baseUrl": "https://api.anthropic.com",
    "model": "claude-3-5-sonnet-20241022",
    "maxTokens": 4096,
    "temperature": 0.7
  },
  "session": {
    "timeout": 1800000,
    "maxSessions": 50,
    "cleanupInterval": 300000
  },
  "tools": {
    "enabled": ["file_reader", "code_analyzer"],
    "projectRoot": "/path/to/your/project"
  },
  "logging": {
    "level": "info",
    "enableConsole": true
  }
} 